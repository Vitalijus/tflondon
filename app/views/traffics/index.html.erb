<%= form_tag('/traffics', method: :get) do %>
  <div>
    <%= label_tag 'Start date' %>
    <%= text_field_tag :start_date, "2016-12-01" %>
  </div>

  <br>

  <div>
    <%= label_tag 'End date' %>
    <%= text_field_tag :end_date, "2016-12-31" %>
  </div>

  <br>

  <%= submit_tag "Submit" %>
<% end %>


<div id="map"></div>
